# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Twilio Configuration (REQUIRED)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Cloudflare RealtimeKit Configuration (REQUIRED for video-commander)
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_REALTIME_ORG_ID=your_realtime_org_id_here
CLOUDFLARE_REALTIME_API_TOKEN=your_realtime_api_token_here
CLOUDFLARE_REALTIME_GLOBAL_ROOM_ID=your_global_room_id_here