{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "lib": ["WebWorker", "ESNext"],
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": ".",
        "types": ["@cloudflare/workers-types"]
    },
    "include": ["src", "scripts"]
}
